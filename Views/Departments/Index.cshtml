@{
  ViewData["Title"] = "Departments";
}

<h1>@ViewData["Title"]</h1>

<div>
  <table class="table table-striped table-borderd">
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
      </tr>
    </thead>

    <tbody>
      @foreach(var department in Model)
      {
        <tr>
          <td>@department["id"]</td>
          <td>@department["Name"]</td>
        </tr>
      }
    </tbody>
  </table>
</div>